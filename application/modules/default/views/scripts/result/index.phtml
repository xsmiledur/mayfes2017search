    function timer(num, map) {
        //最初の画像以外を非表示にする
        //var map = $('#map_img div:first-child');
        var flg;

        //map.after().hide();
        //タイマー

        timer1 = setInterval(function () {
            console.log(map);
            //フェードインとアウトを繰り返す
            if (map.get(0) != undefined) {
                if (map.hasClass('map-comment')) {
                    if (flg == 1) {
                        $('#map_img div').not(':first').hide();
                        map = map.next('div.over-map.well' + num);
                        flg = 0;
                    } else {
                        map.show();
                        flg = 1;
                    }
                } else {
                    map.show();
                    map = map.next('div.over-map.well' + num);
                }
            } else {
                map = $('#map_img div:first-child');
                $('#map_img div').not(':first').hide();
            }
        }, 1500);
    }

