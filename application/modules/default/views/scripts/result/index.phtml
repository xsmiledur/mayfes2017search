<script>
    /* twitterシェア*/
    window.twttr=(function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],t=window.twttr||{};if(d.getElementById(id))return;js=d.createElement(s);js.id=id;js.src="https://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);t._e=[];t.ready=function(f){t._e.push(f);};return t;}(document,"script","twitter-wjs"));

    //ページ遷移の前に警告
    /*$(window).on('beforeunload', function(e) {
        return 'ページを離れると検索結果は保存されない場合があります。このまま移動しますか？';
    });*/

    var timer_;
    var map;
    function timer(num, map) {
        var flg;

        timer_ = setInterval(function () {
            //フェードインとアウトを繰り返す
            if (map[0] == undefined || map[0].get(0) == undefined) {
                map[0] = $('.map-backgd .over-map.well' + num + ':first');
                $('.map-backgd div').not(':first').hide();
            } else {
                if (map[0].hasClass('map-comment')) {
                    if (flg == 1) {
                        $('.map-backgd div').not(':first').hide();
                        map[0] = map[0].next('div.over-map.well' + num);
                        //console.log(map[0]);
                        flg = 0;
                    } else {
                        map[0].show();
                        flg = 1;
                    }
                } else {
                    map[0].show();
                    map[0] = map[0].next('div.over-map.well' + num);
                }
            }

        }, 1500);
    }
    function stopAnimate(num, map) {
        clearInterval(timer_);
        $('.map-backgd .over-map.well' + num).show();
        delete map[0].get(0);
        delete map[0];
        //console.log(map[0]);
        $('.btn-stop').prop('disabled', true);
        $('.btn-pause').prop('disabled', true);
        $('.btn-play').prop('disabled', false);

    }
    function pauseAnimate(num) {
        clearInterval(timer_);
        //console.log("pause");
        //console.log(map[0]);
        $('.btn-pause').prop('disabled', true);
        $('.btn-stop').prop('disabled', true);
        $('.btn-play').prop('disabled', false);

    }
    function playAnimate(num, map) {
        //console.log(map[0]);
        timer(num, map);
        $('.btn-play').prop('disabled', true);
        $('.btn-stop').prop('disabled', false);
        $('.btn-pause').prop('disabled', false);
    }


    $(function () {



        <!-- 地図のルートを点滅画像で表示 -->


        $('.map-backgd div').not(':first').hide();
        if ($(window).width() > 767) {


            map = [$('.map-backgd .over-map.well1:first')];
            timer(1, map);
            $('.btn-play').prop('disabled', true); //最初は再生ボタンdisabled

            $('.btn-stop').click(function() {
                stopAnimate(1, map);
            });
            $('.btn-pause').click(function() {
                pauseAnimate(1);
            });
            $('.btn-play').click(function() {
                playAnimate(1, map);
            });
            $('.btn-print').show();


        } else {

            map = [$('.map-backgd .over-map.well2:first')];
            timer(2, map);
            $('.btn-play').prop('disabled', true); //最初は再生ボタンdisabled

            $('.btn-stop').click(function() {
                stopAnimate(2, map);
            });
            $('.btn-pause').click(function() {
                pauseAnimate(2);
            });
            $('.btn-play').click(function() {
                playAnimate(2, map);
            });
            $('.btn-print').hide();
        }



        //console.log($(window).width());
        if ($(window).width() > 1200) {
            //map等はスクロールしてもついてくる
            $("#sticker").sticky({
                topSpacing: 10,
                bottomSpacing: 90,
                center: true
            });
        }

        var err_msg = $('input[name="err-msg"]').val();
        if (err_msg.length > 0) {
            swal({
                title: err_msg,
                text: "お手数ですが、条件を変更して再度検索してください",
                type: "warning",
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "OK",
                closeOnConfirm: false
            });
        }

        $('.widget-back').click(function () {
            swal({
                title: "Are you OK?",
                text: "入力画面に戻ると検索結果は保存されない場合があります。それでもよろしいですか？",
                type: "warning",
                confirmButtonColor: "#DD6B55",
                confirmButtonText: "OK",
                showCancelButton: true,
                closeOnConfirm: false,
                allowOutsideClick: true,
            }, function() {
                window.location.href = "/"
            });
        });
        $('.widget-vote').hover(function() {
            $('.vote-comment').fadeIn();
            $('.vote-maychan').animate({
                height: "70%",
            });
        },function() {
            $('.vote-comment').fadeOut();
            $('.vote-maychan').animate({
                height: "40%"
            });
        });
        $('.btn-research').click(function() {
            $('#page-wrapper').addClass('sk-loading');
        });
        <?php foreach ($this->project as $key => $item) : ?>
        $("#ionrange<?php echo $key; ?>").ionRangeSlider({
